<app>
 <id>part_liquidation_app_id</id>
 <name>Loan Part Liquidation</name>
 <provider>RenmoneyIT</provider>
 <description>An app to perform part liquidation on loan accounts</description>
 <permission>VIEW_LOAN_ACCOUNT_DETAILS</permission>
 <permission>APPROVE_LOANS</permission>
 <permission>ENTER_REPAYMENT</permission>
 <permission>DIBURSE_LOANS</permission>
 <permission>VIEW_CLIENT_DETAILS</permission>
 <permission>VIEW_DOCUMENTS</permission>
 <permission>CREATE_DOCUMENTS</permission>
 <permission>EDIT_DOCUMENTS</permission>
 <uninstallUrl>https://renbrokerstaging.com/part_liquidation/Loan_account/uninstall</uninstallUrl>
